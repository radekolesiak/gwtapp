<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<display-name>GWT</display-name>
	<filter>
		<filter-name>GZIP</filter-name>
		<filter-class>com.cuprum.filters.GZIPFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>GZIP</filter-name>
		<url-pattern>*.rpc</url-pattern>
	</filter-mapping>
	<filter>
		<filter-name>Proxy</filter-name>
		<filter-class>com.cuprum.filters.ProxyFilter</filter-class>
		<init-param>
      		<param-name>server</param-name>
      		<param-value>http://gwtapp.cuprum.biz:8080/smallapp-app/</param-value>
    	</init-param>		
	</filter>
	<filter-mapping>
		<filter-name>Proxy</filter-name>
		<url-pattern>*.rpc</url-pattern>
	</filter-mapping>
</web-app>
