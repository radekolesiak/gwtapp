<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4">
	<display-name>GWT</display-name>

	<filter>
		<filter-name>GZIP</filter-name>
		<filter-class>com.cuprum.filters.GZIPFilter
		</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>GZIP</filter-name>
		<url-pattern>*.html</url-pattern>
	</filter-mapping>

	<servlet>
		<servlet-name>GWTSpring</servlet-name>
		<servlet-class>com.cuprum.server.common.rpc.RemoteServiceServletSpring
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GWTSpring</servlet-name>
		<url-pattern>*.rpc</url-pattern>
	</servlet-mapping>
</web-app>
